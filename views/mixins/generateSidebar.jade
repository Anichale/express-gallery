mixin generateSidebar(listings)
  .row.show-for-medium-up
    - for (var i = 0; i < 3; i++)
      .medium-12.columns
        .sidebar-container
          a(href='/gallery/'+listings[i].id)
            img.image-listings(src= listings[i].url)
          div.sidebar-info=listings[i].shortDesc + ' - ' +  listings[i].link
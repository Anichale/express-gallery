mixin generateSidebar(listings)
  - for (var i = 0; i < listings.length; i += 6)
    .row.show-for-medium-up
      - for (var j = i; j < 3+i; j++)
        .medium-12.columns
          .image-container
            img.image-listings(src= listings[j].picture)
            div.image-info=listings[j].description + ' ' +  listings[j].link